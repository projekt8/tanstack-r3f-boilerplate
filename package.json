{
  "name": "tanstack-r3f-boilerplate",
  "version": "1.0.0",
  "homepage": "https://projekt8.github.io/tanstack-r3f-boilerplate",
  "description": "A modern, high-performance boilerplate built for the next generation of web experiences. Combining the power of TanStack Start for full-stack capabilities, React Three Fiber for immersive 3D content, and GSAP for award-winning animations.",
  "keywords": [
    "tanstack",
    "react-three-fiber",
    "gsap",
    "lenis",
    "tempus",
    "react",
    "tailwindcss",
    "typescript",
    "vite"
  ],
  "author": {
    "name": "Kerim Hadri",
    "homepage": "https://projekt8.net"
  },
  "engines": {
    "node": ">=24.8.0"
  },
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier",
    "format:fix": "prettier --write .",
    "check": "npm run lint:fix && npm run format:fix",
    "optimize:models": "tsx scripts/optimize-models/index.ts"
  },
  "dependencies": {
    "@gsap/react": "2.1.2",
    "@react-three/drei": "10.7.7",
    "@react-three/fiber": "9.5.0",
    "@tailwindcss/vite": "4.2.0",
    "@tanstack/react-devtools": "0.9.6",
    "@tanstack/react-router": "1.161.1",
    "@tanstack/react-router-devtools": "1.161.1",
    "@tanstack/react-router-ssr-query": "1.161.1",
    "@tanstack/react-start": "1.161.1",
    "@tanstack/router-plugin": "1.161.1",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "gsap": "3.14.2",
    "lenis": "1.3.17",
    "lucide-react": "0.575.0",
    "nitro": "npm:nitro-nightly@latest",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "tailwind-merge": "3.5.0",
    "tailwindcss": "4.2.0",
    "tempus": "1.0.0-dev.17",
    "three": "0.183.0",
    "vite-tsconfig-paths": "6.1.1",
    "zustand": "5.0.11"
  },
  "devDependencies": {
    "@fontsource-variable/inter": "5.2.8",
    "@fontsource-variable/playfair-display": "5.2.8",
    "@pmndrs/assets": "1.7.0",
    "@tanstack/devtools-vite": "0.5.1",
    "@tanstack/eslint-config": "0.4.0",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.2",
    "@types/node": "22.19.11",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.4",
    "commander": "14.0.3",
    "jsdom": "27.4.0",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "ts-morph": "27.0.2",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-glsl": "1.5.5",
    "vite-plugin-svgr": "4.5.0",
    "vitest": "3.2.4"
  }
